{
  "name": "gamehub",
  "version": "1.0.0",
  "description": "GameHub - Multiplayer Game Rooms",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "nodemon server.js",
    "dev:client": "vite --config client/vite.config.js",
    "build": "vite build --config client/vite.config.js"
  },
  "dependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^8.10.1",
    "passport": "^0.7.0",
    "passport-discord": "^0.1.4",
    "passport-google-oauth20": "^2.0.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.3",
    "uuid": "^11.0.5",
    "vite": "^6.4.1"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.11"
  }
}
